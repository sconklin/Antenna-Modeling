def inches(meters):
    return float(meters * 39.3700787)

def meters(inches):
    return float(inches / 39.3700787)

def main():

    topdia = 0.25 # Top element diameter in inches
    botdia = 0.25 # Bottom element diameter in inches
    enddia = 0.537 # End shorting bar diameter
    topoff = 0.7625 # offset of top element in inches - .7625 clears boom with 1/4" dia elements, .950 clears with 5/8
    botoff = -0.7625 
    drivenlength = 36.7

    rightend_Y = "%1.5f" % (meters(drivenlength)/2)
    leftend_Y = "%1.5f" % (-1.0*meters(drivenlength)/2)
    top_Z = "%1.5f" %  meters(topoff)
    bot_Z = "%1.5f" %  meters(botoff)
    top_DIA =  "%1.5f" %  meters(topdia / 2)
    bot_DIA =  "%1.5f" %  meters(botdia / 2)
    end_DIA =  "%1.5f" %  meters(enddia / 2)

    print ("CM Test generated by a python script")
    print ("CM     topdia = %1.5f in" % topdia)
    print ("CM     botdia = %1.5f in" % botdia)
    print ("CM     enddia = %1.5f in" % enddia)
    print ("CM     topoff = %1.5f in" % topoff)
    print ("CM     botoff = %1.5f in" % botoff)
    print ("CM     drivenlength = %1.5f in" % drivenlength)
    
    print ("CE End Comments")
    # Top element (driven)
    # print("GW     1    25  3.98000E-01  4.85000E-01  1.00000E-02  3.98000E-01 -4.85000E-01  1.00000E-02  7.94000E-03")
    print("GW     1    25  3.98000E-01  %s  %s  3.98000E-01 %s  %s  %s" % (leftend_Y, top_Z, rightend_Y, top_Z, top_DIA))
    # Bottom Element
    # print("GW     2    25  3.98000E-01  4.85000E-01  -1.00000E-02  3.98000E-01 -4.85000E-01 -1.00000E-02  7.94000E-03")
    print("GW     2    25  3.98000E-01  %s  %s  3.98000E-01 %s  %s  %s" % (leftend_Y, bot_Z, rightend_Y, bot_Z, bot_DIA))
    #Shorting bars
    # print("GW     3     1  3.98000E-01  4.85000E-01  1.00000E-02 3.98000E-01  4.85000E-01  -1.00000E-02  7.94000E-03")
    print("GW     3     1  3.98000E-01  %s  %s 3.98000E-01  %s  %s  %s" % (leftend_Y, top_Z, leftend_Y, bot_Z, end_DIA ))
    # print("GW     4     1  3.98000E-01 -4.85000E-01  1.00000E-02 3.98000E-01 -4.85000E-01 -1.00000E-02  7.94000E-03")
    print("GW     4     1  3.98000E-01  %s  %s 3.98000E-01  %s  %s  %s" % (rightend_Y, top_Z, rightend_Y, bot_Z, end_DIA ))

    print("GM     0     0  0.00000E+00  0.00000E+00  0.00000E+00 0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00")
    print("GE     0     0  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00")
    print("FR     0    40     0      0  1.44000E+02  1.00000E-01  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00")
    print("EX     0     1    13      0  1.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00")
    print("LD     5     0     0      0  3.70000E+07  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00")
    print("NH     0     0     0      0  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00")
    print("NE     0    20    15      1 -1.40000E+00 -1.40000E+00  5.00000E-02  2.00000E-01  2.00000E-01  0.00000E+00")
    print("RP     0    37    73   1000  0.00000E+00  0.00000E+00  5.00000E+00  5.00000E+00  0.00000E+00  0.00000E+00")
    print("EN     0     0     0      0  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00  0.00000E+00")

    return

main()

